root = true

[*]
indent_style = space
end_of_line = lf
charset = utf-8
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true

[*.ts]
quote_type = single

[*.tsx]
quote_type = double

[*.js]
quote_type = single

[*.jsx]
quote_type = double

[*.md]
trim_trailing_whitespace = false
