{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "npm run lint --fix",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "precommit": "lint-staged",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "npx firebase emulators:start --import=./firebase/firestore-data",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.7",
    "@mui/material": "5.14.7",
    "@mui/x-data-grid": "6.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.1.1",
    "react-router-dom": "6.15.0"
  },
  "devDependencies": {
    "@mdx-js/preact": "2.3.0",
    "@storybook/addon-essentials": "7.3.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "7.3.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/builder-vite": "7.3.2",
    "@storybook/react": "7.3.2",
    "@storybook/react-vite": "7.3.2",
    "@storybook/testing-library": "0.2.0",
    "@testing-library/jest-dom": "6.1.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.8",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "@vitejs/plugin-react": "4.0.4",
    "babel-jest": "29.6.4",
    "babel-loader": "9.1.3",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "esbuild-register": "3.4.2",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "3.0.0",
    "firebase": "10.3.1",
    "jest": "29.6.4",
    "jest-environment-jsdom": "29.6.4",
    "jest-resolve": "29.6.4",
    "jest-watch-typeahead": "2.2.2",
    "prettier": "3.0.3",
    "prop-types": "^15.8.1",
    "rc": "1.2.8",
    "rollup-plugin-visualizer": "5.9.2",
    "storybook": "^7.0.2",
    "storybook-dark-mode": "3.0.1",
    "ts-jest": "29.1.1",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-checker": "0.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}