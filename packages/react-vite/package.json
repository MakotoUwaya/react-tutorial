{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "biome format ./src --write",
    "lint": "biome lint ./src --files-ignore-unknown=true",
    "lint:fix": "biome check --apply ./src --files-ignore-unknown=true",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "npx firebase emulators:start --import=./firebase/firestore-data",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.4.0",
    "@mui/material": "6.4.0",
    "@mui/x-data-grid": "7.23.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-firebase-hooks": "5.1.1",
    "react-router-dom": "7.1.2"
  },
  "devDependencies": {
    "@mdx-js/preact": "3.1.0",
    "@storybook/addon-essentials": "8.5.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "8.5.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/builder-vite": "8.5.0",
    "@storybook/react": "8.5.0",
    "@storybook/react-vite": "8.5.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.6",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@vitejs/plugin-react": "4.3.4",
    "babel-jest": "29.7.0",
    "babel-loader": "9.2.1",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "esbuild-register": "3.6.0",
    "firebase": "11.1.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-resolve": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "prop-types": "^15.8.1",
    "rc": "1.2.8",
    "rollup-plugin-visualizer": "5.14.0",
    "storybook": "^8.0.0",
    "storybook-dark-mode": "4.0.2",
    "ts-jest": "29.2.5",
    "typescript": "5.7.3",
    "vite": "6.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
