{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "npm run lint --fix",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "precommit": "lint-staged",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "firebase emulators:start --import=./firebase/firestore-data"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.8.0",
    "@mui/material": "5.8.1",
    "@mui/x-data-grid": "5.11.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "5.0.3",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.2.0",
    "@types/jest": "27.5.1",
    "@types/node": "16.11.36",
    "@types/react": "17.0.45",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "@vitejs/plugin-react": "1.3.2",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.5",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "firebase": "9.8.2",
    "firebase-tools": "11.0.1",
    "jest": "27.5.1",
    "jest-resolve": "27.5.1",
    "jest-watch-typeahead": "1.1.0",
    "lint-staged": "12.4.2",
    "prettier": "2.6.2",
    "rc": "1.2.8",
    "react-scripts": "5.0.1",
    "rollup-plugin-visualizer": "5.6.0",
    "typescript": "4.6.4",
    "vite": "2.9.9",
    "vite-plugin-checker": "0.4.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}