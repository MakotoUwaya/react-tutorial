{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "npm run lint --fix",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "precommit": "lint-staged",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "firebase emulators:start --import=./firebase/firestore-data",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@emotion/react": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.11.16",
    "@mui/x-data-grid": "6.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-firebase-hooks": "5.1.1",
    "react-router-dom": "6.10.0"
  },
  "devDependencies": {
    "@mdx-js/preact": "2.2.1",
    "@storybook/addon-essentials": "7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/builder-vite": "7.0.2",
    "@storybook/react": "7.0.2",
    "@storybook/react-vite": "7.0.2",
    "@storybook/testing-library": "0.1.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.2.6",
    "@types/node": "18.11.19",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "@vitejs/plugin-react": "2.2.0",
    "babel-jest": "29.3.1",
    "babel-loader": "9.1.2",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "esbuild-register": "3.4.2",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-unused-imports": "2.0.0",
    "firebase": "9.16.0",
    "firebase-tools": "11.21.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "jest-resolve": "29.3.1",
    "jest-watch-typeahead": "2.2.2",
    "prettier": "2.8.7",
    "prop-types": "^15.8.1",
    "rc": "1.2.8",
    "rollup-plugin-visualizer": "5.9.0",
    "storybook": "^7.0.2",
    "storybook-dark-mode": "3.0.0",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5",
    "vite": "3.2.5",
    "vite-plugin-checker": "0.5.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
