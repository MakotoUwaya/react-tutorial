{
  "name": "react-vite",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "lint:fix": "npm run lint --fix",
    "dev": "vite",
    "dev:emulator": "vite --mode emulator",
    "precommit": "lint-staged",
    "build": "tsc --noEmit && vite build",
    "analyze": "ANALYZE=true npm run build",
    "serve": "vite preview",
    "test": "jest",
    "test:updateSnapshot": "npm run test -- -u",
    "emulators:start": "firebase emulators:start --import=./firebase/firestore-data",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@emotion/react": "11.10.0",
    "@emotion/styled": "11.10.0",
    "@mui/icons-material": "5.10.2",
    "@mui/material": "5.10.2",
    "@mui/x-data-grid": "5.16.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebase-hooks": "5.0.3",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@mdx-js/preact": "2.1.5",
    "@storybook/addon-a11y": "6.5.10",
    "@storybook/addon-actions": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/addon-interactions": "6.5.10",
    "@storybook/addon-links": "6.5.10",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/node-logger": "6.5.10",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.10",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "28.1.4",
    "@types/node": "16.11.68",
    "@types/react": "17.0.52",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.35.1",
    "@typescript-eslint/parser": "5.35.1",
    "@vitejs/plugin-react": "2.0.1",
    "babel-jest": "29.2.1",
    "babel-loader": "8.2.5",
    "babel-plugin-named-asset-import": "0.3.8",
    "babel-preset-react-app": "10.0.1",
    "esbuild-register": "3.3.3",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "firebase": "9.9.4",
    "firebase-tools": "11.8.0",
    "jest": "29.2.1",
    "jest-environment-jsdom": "29.2.1",
    "jest-resolve": "29.2.1",
    "jest-watch-typeahead": "2.1.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "rc": "1.2.8",
    "react-scripts": "5.0.1",
    "rollup-plugin-visualizer": "5.8.0",
    "storybook-dark-mode": "1.1.0",
    "ts-jest": "29.0.3",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vite-plugin-checker": "0.5.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
