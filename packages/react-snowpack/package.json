{
  "name": "react-snowpack",
  "version": "0.0.3",
  "description": "React snowpack sample",
  "scripts": {
    "start": "snowpack dev --polyfill-node",
    "build": "snowpack build",
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx . && prettier --write '**/*.{ts,tsx}'",
