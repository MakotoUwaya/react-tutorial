{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "react-vite on chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000/webapp-template/",
      "webRoot": "${workspaceFolder}/packages/react-vite/src",
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
