{
  "name": "webapp-template",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "5.0.0",
    "lint-staged": "13.0.0",
    "prettier": "2.6.2",
    "turbo": "1.2.16"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "check": "turbo run lint build test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "test:local": "lerna run test --since $(git show-branch | grep '*' | grep -v \"$(git rev-parse --abbrev-ref HEAD)\" | head -1 | awk -F'[]~^[]' '{print $2}')",
    "build": "turbo run build"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.12.1"
  },
  "workspaces": [
    "packages/*"
  ]
}