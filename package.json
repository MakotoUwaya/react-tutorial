{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "packageManager": "npm@8.6.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "4.0.0",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "turbo": "1.1.10"
  },
  "scripts": {
    "bootstrap": "npm ci && lerna bootstrap",
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "versionup": "lerna version --no-push --tag-version-prefix=''",
    "check": "turbo run lint build test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "build": "turbo run build"
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.6.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
