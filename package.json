{
  "name": "webapp-template",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "6.4.1",
    "lint-staged": "13.1.4",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "turbo": "1.8.8"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "check": "turbo run lint build test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "test:local": "lerna run test --since main",
    "build": "turbo run build"
  },
  "volta": {
    "node": "18.15.0",
    "npm": "9.3.1"
  },
  "workspaces": [
    "packages/*"
  ]
}
