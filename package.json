{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "eslint": "7.28.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-unused-imports": "1.1.1",
    "husky": "6.0.0",
    "lerna": "4.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1"
  },
  "lint-staged": {
    "**/*.{jsx,js,tsx,ts}": [
      "npm run lint"
    ]
  },
  "scripts": {
    "npm-ci": "npm ci && lerna bootstrap",
    "prepare": "husky install",
    "versionup": "lerna version --force-publish",
    "versionup:patch": "lerna version patch --force-publish",
    "versionup:minor": "lerna version minor --force-publish",
    "versionup:major": "lerna version major --force-publish",
    "lint": "lerna run --stream lint",
    "test": "lerna run --stream test",
    "webapp:test": "lerna run --stream --scope react-tutorial-webapp test",
    "build": "lerna run --stream build",
    "webapp:build": "lerna run --stream --scope react-tutorial-webapp build",
    "webapp:start": "lerna run --stream --scope react-tutorial-webapp start",
    "webapp:storybook": "lerna run --stream --scope react-tutorial-webapp storybook"
  }
}