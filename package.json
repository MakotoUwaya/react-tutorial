{
  "name": "webapp-template",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.32.0",
    "@typescript-eslint/parser": "5.32.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "5.4.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "turbo": "1.4.3"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "check": "turbo run lint build test",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "test:local": "lerna run test --since main",
    "build": "turbo run build"
  },
  "volta": {
    "node": "16.17.0",
    "npm": "8.15.1"
  },
  "workspaces": [
    "packages/*"
  ]
}