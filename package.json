{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "packageManager": "npm@8.5.2",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "4.0.0",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "turbo": "1.1.3"
  },
  "scripts": {
    "postinstall": "lerna bootstrap",
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "versionup": "lerna version --no-push --tag-version-prefix=''",
    "lint": "lerna run --stream lint",
    "lint:fix": "lerna run --stream lint:fix",
    "test": "lerna run --stream test",
    "test:updateSnapshot": "lerna run --stream test:updateSnapshot",
    "build": "lerna run --stream build"
  },
  "volta": {
    "node": "16.14.0",
    "npm": "8.5.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
