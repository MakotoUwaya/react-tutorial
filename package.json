    "test": "turbo run test",
    "test:updateSnapshot": "turbo run test:updateSnapshot",
    "build": "turbo run build"
  },
  "packageManager": "npm@10.8.3",
  "volta": {
    "node": "20.17.0",
    "npm": "10.8.3"
  },
  "workspaces": [
    "packages/*"
  ]
