{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-unused-imports": "2.0.0",
    "lerna": "4.0.0",
    "lint-staged": "12.3.2",
    "prettier": "2.5.1"
  },
  "scripts": {
    "bootstrap": "npm ci && lerna bootstrap",
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "versionup": "lerna version --no-push --tag-version-prefix=''",
    "lint": "lerna run --stream lint",
    "lint:fix": "lerna run --stream lint:fix",
    "test": "lerna run --stream test",
    "test:updateSnapshot": "lerna run --stream test:updateSnapshot",
    "apps:test": "lerna run --stream --scope react-tutorial-apps test",
    "build": "lerna run --stream build",
    "apps:build": "lerna run --stream --scope react-tutorial-apps build",
    "apps:start": "lerna run --stream --scope react-tutorial-apps start",
    "apps:storybook": "lerna run --stream --scope react-tutorial-apps storybook"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.3.2"
  }
}
