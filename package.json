{
  "name": "react-tutorial",
  "version": "0.0.1",
  "description": "tutorial project",
  "private": true,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-unused-imports": "1.1.4",
    "lerna": "4.0.0",
    "lint-staged": "11.1.2",
    "prettier": "2.3.2"
  },
  "scripts": {
    "bootstrap": "npm ci && lerna bootstrap",
    "prepare": "git config --local core.hooksPath .githook || echo 'Can not set git hooks' ",
    "versionup": "lerna version --no-push --tag-version-prefix=''",
    "versionup:patch": "lerna version patch --no-push --tag-version-prefix=''",
    "versionup:minor": "lerna version minor --no-push --tag-version-prefix=''",
    "versionup:major": "lerna version major --no-push --tag-version-prefix=''",
    "lint": "lerna run --stream lint",
    "lint:fix": "lerna run --stream lint:fix",
    "test": "lerna run --stream test",
    "test:updateSnapshot": "lerna run --stream test:updateSnapshot",
    "webapp:test": "lerna run --stream --scope react-tutorial-webapp test",
    "build": "lerna run --stream build",
    "webapp:build": "lerna run --stream --scope react-tutorial-webapp build",
    "webapp:start": "lerna run --stream --scope react-tutorial-webapp start",
    "webapp:storybook": "lerna run --stream --scope react-tutorial-webapp storybook"
  }
}
